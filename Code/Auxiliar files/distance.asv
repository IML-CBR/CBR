function [ dist ] = distance(att_new, att_old)
    % If wether 
    if isnan(att_new) || isnan(att_old) || ~xor(isnumeric(att_new),isnumeric(att_old))
        dist = 1;
    elseif isnumeric(att_new)
        % Euclidean distance
        dist = (sum((att_new-att_old).^2)).^0.5;
    else
        dist = ~strcmp(att_new, att_old);
    end
end